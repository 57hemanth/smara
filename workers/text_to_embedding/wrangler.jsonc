{
	"name": "smara-text-to-embedding",
	"main": "index.ts",
	"compatibility_date": "2025-10-01",
	"ai": {
		"binding": "AI",
		"remote": true
	},
	"vectorize": [
		{
			"binding": "VECTORIZE",
			"index_name": "smara-index",
			"remote": true
		}
	],
	"queues": {
		"consumers" : [{
			"queue": "smara-embedding-queue",
			"max_batch_size": 1,
			"max_batch_timeout": 0
		}]
	}
}