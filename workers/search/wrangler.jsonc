{
    "name": "smara-search",
    "main": "index.ts",
    "compatibility_date": "2025-10-01",
    "vectorize": [
		{
			"binding": "VECTORIZE",
			"index_name": "smara-index",
			"remote": true
		}
	],
	"ai": {
		"binding": "AI",
		"remote": true
	},
	"observability": {
        "logs": {
            "enabled": true,
            "head_sampling_rate": 1,
            "invocation_logs": true
        }
    }
}